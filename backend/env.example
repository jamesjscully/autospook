# AutoSpook OSINT Environment Configuration
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://osint_user:osint_secure_pass_2024@localhost:5432/osint_db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=osint_db
POSTGRES_USER=osint_user
POSTGRES_PASSWORD=osint_secure_pass_2024

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379

# LangGraph Configuration
LANGSMITH_API_KEY=your_langsmith_api_key_here

# AI Model API Keys (Required)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Search API Keys (Required for web search)
GOOGLE_SEARCH_API_KEY=your_google_search_api_key_here
GOOGLE_CSE_ID=your_google_custom_search_engine_id_here

# Optional: Additional Search APIs
BING_API_KEY=your_bing_api_key_here
SERPAPI_KEY=your_serpapi_key_here

# Application Settings
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO
SECRET_KEY=your_secret_key_here

# OSINT Specific Settings
MAX_RETRIEVALS_PER_INVESTIGATION=20
MAX_ENTITIES_PER_INVESTIGATION=100
CACHE_TTL_SECONDS=3600
RATE_LIMIT_PER_MINUTE=60

# Security Settings (Optional)
ENABLE_ENCRYPTION=false
ENCRYPTION_KEY=your_encryption_key_here
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Monitoring (Optional)
SENTRY_DSN=your_sentry_dsn_here
DATADOG_API_KEY=your_datadog_api_key_here
ENABLE_PHOENIX=false
